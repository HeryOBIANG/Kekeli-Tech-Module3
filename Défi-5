for(int i = 0; i <= 100; i++)
{
    if (i % 3 == 0 && i % 5 == 0)
    Console.WriteLine($"{i} - FizzBuzz");
    else if(i % 3 == 0)
    Console.WriteLine($"{i} - Fizz");
    else if(i % 5 == 0)
    Console.WriteLine($"{i} - Buzz");
    else
    Console.WriteLine(i);
}


//Liens énoncé: https://learn.microsoft.com/fr-fr/training/modules/csharp-for/3-challenge
//FizzBuzz affiche les multiples de 3 et 5 
